{
	"name": "autarky/framework",
	"description": "Autarky - PHP framework for self-sufficient developers",
	"keywords": ["autarky", "framework"],
	"license": "MIT",
	"authors": [
		{
			"name": "Andreas Lutro",
			"email": "anlutro@gmail.com"
		}
	],
	"require": {
		"symfony/http-kernel": "2.4.*",
		"symfony/http-foundation": "2.4.*",
		"symfony/browser-kit": "2.4.*",
		"symfony/console": "2.4.*",
		"stack/builder": "1.0.*",
		"nikic/fast-route": "0.1.*",
		"illuminate/container": "4.1.*",
		"twig/twig": "1.15.*",
		"monolog/monolog": "1.8.*"
	},
	"require-dev": {
		"phpunit/phpunit": "4.*",
		"mockery/mockery": "0.9.*"
	},
	"autoload": {
		"psr-4": {
			"Autarky\\": "src/"
		},
		"files": [
			"src/functions.php"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.2-dev"
		}
	},
	"minimum-stability": "stable"
}
