{
	"name": "autarky/framework",
	"description": "Autarky - PHP framework for self-sufficient developers",
	"keywords": ["autarky", "framework"],
	"license": "MIT",
	"authors": [
		{
			"name": "Andreas Lutro",
			"email": "anlutro@gmail.com"
		}
	],
	"require": {
		"autarky/container": "~1.2",
		"nikic/fast-route": "~1.0",
		"psr/log": "~1.0",
		"stack/builder": "~1.0",
		"symfony/http-foundation": "~2.4|~3.0",
		"symfony/http-kernel": "~2.4|~3.0"
	},
	"suggest": {
		"autarky/twig-templating": "Twig templating integration (removed from framework package in 0.8)"
	},
	"require-dev": {
		"autarky/twig-templating": "~0.2",
		"mockery/mockery": "^0.9.1",
		"monolog/monolog": "~1.3",
		"phpunit/phpunit": "~4.8|~5.0",
		"symfony/browser-kit": "~2.3|~3.0",
		"symfony/console": "~2.3|~3.0",
		"symfony/yaml": "~2.3|~3.0",
		"symfony/polyfill-php70": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"Autarky\\": "classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Autarky\\Tests\\": "tests/classes/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-develop": "0.11-dev",
			"dev-master": "0.10-dev",
			"dev-0.9": "0.9-dev",
			"dev-0.8": "0.8-dev"
		}
	},
	"minimum-stability": "dev"
}
