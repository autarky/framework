{
	"name": "autarky/framework",
	"description": "Autarky - PHP framework for self-sufficient developers",
	"keywords": ["autarky", "framework"],
	"license": "MIT",
	"authors": [
		{
			"name": "Andreas Lutro",
			"email": "anlutro@gmail.com"
		}
	],
	"require": {
		"symfony/http-kernel": "2.5.*",
		"symfony/http-foundation": "2.5.*",
		"symfony/browser-kit": "2.5.*",
		"symfony/console": "2.5.*",
		"stack/builder": "1.0.*",
		"nikic/fast-route": "0.2.*",
		"twig/twig": "1.16.*",
		"monolog/monolog": "1.10.*"
	},
	"require-dev": {
		"phpunit/phpunit": "4.*",
		"mockery/mockery": "0.9.*"
	},
	"autoload": {
		"psr-4": {
			"Autarky\\": "src/"
		},
		"files": [
			"src/functions.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Autarky\\Tests\\": "tests/src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-0.3": "0.3-dev",
			"dev-0.2": "0.2-dev",
			"dev-0.1": "0.1-dev"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
