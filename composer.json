{
	"name": "autarky/framework",
	"description": "Autarky - PHP framework for self-sufficient developers",
	"keywords": ["autarky", "framework"],
	"license": "MIT",
	"authors": [
		{
			"name": "Andreas Lutro",
			"email": "anlutro@gmail.com"
		}
	],
	"require": {
		"symfony/http-kernel": "~2.4",
		"symfony/http-foundation": "~2.4",
		"symfony/browser-kit": "~2.3",
		"symfony/console": "~2.3",
		"stack/builder": "~1.0",
		"nikic/fast-route": "0.3.*",
		"twig/twig": "~1.16",
		"psr/log": "~1.0"
	},
	"require-dev": {
		"mockery/mockery": "~0.9.1",
		"symfony/yaml": "~2.3"
	},
	"autoload": {
		"psr-4": {
			"Autarky\\": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-develop": "0.8-dev",
			"dev-master": "0.7-dev",
			"dev-0.6": "0.6-dev",
			"dev-0.5": "0.5-dev",
			"dev-0.4": "0.4-dev",
			"dev-0.3": "0.3-dev",
			"dev-0.2": "0.2-dev",
			"dev-0.1": "0.1-dev"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
